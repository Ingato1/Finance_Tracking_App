{
  "version": 2,
  "buildCommand": "python manage.py collectstatic --noinput && python manage.py migrate",
  "builds": [
    {
      "src": "vercel_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/vercel_app.py"
    }
  ]
}
