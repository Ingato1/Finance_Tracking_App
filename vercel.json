{
  "version": 2,
  "buildCommand": "python manage.py collectstatic --noinput && python manage.py migrate",
  "builds": [
    {
      "src": "app/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "app/wsgi.py"
    }
  ]
}
